{
  "name": "ai-newsroom-frontend",
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/frontend/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@api_url"
  }
}